{
    "name": "turbo124/bot-licker",
    "description": "Package for Laravel to block IPs at the network edge",
    "type": "library",
    "keywords": [
        "bots",
        "firewall",
        "cloudflare"
    ],
    "require": {
        "illuminate/support": "^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Turbo124\\BotLicker\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Bomba",
            "email": "turbo124@gmail.com"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Turbo124\\BotLicker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Turbo124\\BotLicker\\FirewallServiceProvider"
            ],
            "aliases": {
                "Firewall": "Turbo124\\BotLicker\\FirewallFacade"
            }
        }
    }
}
